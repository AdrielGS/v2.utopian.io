@import "~variables"

.u-page-contributions {
  .categories-list {
    padding 0 0 5px 0
  }

  .q-layout-page.u-page {
    padding 0 !important
  }

  .q-select, .q-select:hover {
    background-color #fff
    padding 5px
    border 1px solid $grey-4 !important

  }

  .q-list.posts-list {
    padding 0
    .q-item {
      border-radius 4px
      background white !important
      border 1px solid $grey-4
      margin-bottom 15px
      padding-top 15px
      padding-bottom 15px
    }
  }
}
