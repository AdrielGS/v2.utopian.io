{
  "name": "testing",
  "version": "1.0.0",
  "description": "Common package for running a barrage of tests for Quasar apps",
  "keywords": [
    "jest",
    "webdriver",
    "cypress",
    "lighthouse",
    "snyk",
    "unit test",
    "e2e test"
  ],
  "author": "Utopian Company SRLs",
  "homepage": "https://utopian.io",
  "license": "GPL-3.0",
  "main": "lib/testing.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib/testing.js",
    "lib/quasar-test.js"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utopian-io/v2.utopian.io.git"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue lib --fix",
    "test": "jest --coverage",
    "test:jest": "jest --coverage",
    "test:snyk": "snyk test --json > ./test/snyk/snyk_results.json",
    "test:lighthouse": "lighthouse http://localhost:8080",
    "test:webdriver:phantomjs": "webdriver --config ./test/webdriver/wdio.phantomjs.conf.js",
    "test:cypress:dev": "cypress open",
    "test:cypress:e2e": "cypress run",
    "coverage:serve": "quasar serve ./__tests__/coverage/lcov-report",
    "precommit": "lint-staged"
  },
  "bugs": {
    "url": "https://github.com/utopian-io/v2.utopian.io/issues"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "chai": "^4.2.0",
    "chokidar": "^2.0.4",
    "enhanced-resolve": "^4.1.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "fs-extra": "^7.0.0",
    "fs-filesystem": "^2.1.2",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "lighthouse": "^3.2.1",
    "lint-staged": "^7.2.2",
    "minimist": "^1.2.0",
    "promise": "^8.0.2",
    "quasar-cli": "^0.17.20",
    "ramda": "^0.25.0",
    "snyk": "^1.106.1",
    "vue-jest": "^2.6.0",
    "vue-test-helpers": "^2.0.0",
    "watchpack": "^1.6.0",
    "wdio-phantomjs-service": "~0.1",
    "wdio-selenium-standalone-service": "^5.0.0-beta.3",
    "wdio-testingbot-service": "^0.2.1"
  }
}
